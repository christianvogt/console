{
  "name": "@redhat-developer/dev-sandbox-console-plugin",
  "description": "Provides an OpenShift console plugin for capturing events in segment.io.",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "yarn clean && NODE_ENV=production yarn ts-node ./node_modules/.bin/webpack --mode=production",
    "build-dev": "yarn clean && yarn ts-node ./node_modules/.bin/webpack --mode=development",
    "http-server": "./http-server.sh ./dist",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}' -I '/node_modules/(?!(@console)/)/'"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "http-server": "0.12.x"
  },
  "devDependencies": {
    "@console/dynamic-plugin-sdk": "link:../packages/console-dynamic-plugin-sdk",
    "copy-webpack-plugin": "5.1.x",
    "ts-loader": "6.2.2",
    "ts-node": "5.0.1",
    "typescript": "3.8.3",
    "webpack": "5.0.0-beta.16",
    "webpack-cli": "4.5.x"
  },
  "consolePlugin": {
    "name": "dev-sandbox-console-plugin",
    "version": "0.0.0",
    "displayName": "Dev Sandbox Console Plugin",
    "description": "Provides an OpenShift console plugin for capturing events in segment.io.",
    "exposedModules": {
      "telemetry": "./utils/telemetry"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    }
  }
}
